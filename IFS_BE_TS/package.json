{
  "name": "ifs_be_ts",
  "version": "1.0.0",
  "description": "beckend with typescript",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx --watch --inspect=0.0.0.0 src/index.ts",
    "lint": "eslint --fix ./src",
    "format": "prettier ./src --write",
    "prepare": "husky",
    "db:migrate": "tsx ./node_modules/.bin/sequelize db:migrate",
    "db:seed": "tsx ./node_modules/.bin/sequelize db:seed:all",
    "db:migrate:undo": "tsx ./node_modules/.bin/sequelize db:migrate:undo",
    "db:seed:undo": "tsx ./node_modules/.bin/sequelize db:seed:undo:all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "prettier": "^3.3.3",
    "sequelize": "^6.37.3",
    "tsx": "^4.16.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.15",
    "@types/nodemailer": "^6.4.15",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.3",
    "lint-staged": "^15.2.7",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "*.ts": [
      "npm run format",
      "npm run lint"
    ]
  }
}
